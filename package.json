{
    "name": "ai-tracker",
    "displayName": "AI Tracker",
    "description": "Detects AI vs Human code contributions (Copilot, ChatGPT, Claude, Gemini, Codeium, QodoGen, Windsurf).",
    "version": "1.0.2",
    "publisher": "Mansoor Ul Hasan",
    "engines": {
      "vscode": "^1.75.0"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/mansurali901/ai-code-detector-vscode"
    },
    "icon": "extension/images/icon.png",
    "main": "./out/extension.js",
  
    "activationEvents": [
      "onStartupFinished"
    ],
  
    "contributes": {
      "commands": [
        {
          "command": "aianalytics.showStats",
          "title": "AI Analytics: Show Stats",
          "category": "AI Analytics"
        }
      ]
    },
  
    "scripts": {
      "vscode:prepublish": "npm run compile",
      "compile": "tsc -p ./",
      "watch": "tsc -watch -p ./"
    },
  
    "devDependencies": {
      "@types/vscode": "^1.75.0",
      "@types/node": "^20.0.0",
      "typescript": "^5.3.0",
      "eslint": "^8.0.0",
      "vsce": "^2.15.0"
    },
  
    "categories": [
      "Other",
      "Programming Languages",
      "Linters"
    ],
  
    "keywords": [
      "AI",
      "Copilot",
      "ChatGPT",
      "Claude",
      "Gemini",
      "Codeium",
      "Developer Analytics",
      "Productivity"
    ]
  }
  